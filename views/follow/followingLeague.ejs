<!-- follow/followingLeague previous codes are in the NOTE -->

<div class="followingHeaderText">
    <h1>Your Following Leagues</h1>
</div>
<div class="leagueListForLoopContainer">
    <% for( let i = 0; i < storedLeaugeInfos.length; i++ ) { %>
        <div class="leagueListForLoop">
            <h2 id="leagueName"><%= storedLeaugeInfos[i].leaguename %> </h2>
            <h2 >Followed on <%= storedLeaugeInfos[i].createdAt %> </h2>

            <form method="POST" action="/follow/league/<%= storedLeaugeInfos[i].leagueid %>/?_method=DELETE">
                <input type="hidden" name="leagueIdNum" value="<%= storedLeaugeInfos[i].leagueid %>">
                <input type="submit" value="UNFOLLOW">
            </form>
        </div>
    <% } %>
</div>